<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.MainApplication.Controller.ProductController">
   <children>
      <ListView prefHeight="400.0" prefWidth="130.0" />
      <Label prefHeight="40.0" prefWidth="130.0" text="Category">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Label>
      <Button fx:id="cateBtn" layoutY="40.0" mnemonicParsing="false" onAction="#foodButton" prefHeight="40.0" prefWidth="130.0" text="Food" />
      <Button fx:id="cateBtn1" layoutY="80.0" mnemonicParsing="false" onAction="#computerButton" prefHeight="40.0" prefWidth="130.0" text="Computer" />
      <Button fx:id="cateBtn11" layoutY="120.0" mnemonicParsing="false" onAction="#clothButton" prefHeight="40.0" prefWidth="130.0" text="Cloth" />
      <Button fx:id="cateBtn111" layoutY="160.0" mnemonicParsing="false" onAction="#accessoryButton" prefHeight="40.0" prefWidth="130.0" text="Accessory" />
      <Button fx:id="cateBtn1111" layoutY="200.0" mnemonicParsing="false" onAction="#backButton" prefHeight="40.0" prefWidth="130.0" text="Back" />
   </children>
</AnchorPane>
